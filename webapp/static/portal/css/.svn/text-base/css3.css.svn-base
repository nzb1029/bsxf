@charset "utf-8";
/* CSS Document */

@import url("head.css");
.colorb{ color:#7593b0;}
.pl120{ padding-left:120px;}

.mainlogin{ background:url(../images/lbg.jpg) repeat-x #FFF; margin:10px 3px 0 2px;width:997px;border-top:1px solid #bbf4fe; border-bottom:1px solid #e0e0e0;border-left:1px solid #e0e0e0;border-right:1px solid #e0e0e0;}
.login{ margin:0 82px 0 62px; width:858px; padding-bottom:162px;}
.loginlogo{width:487px; height:78px; border-bottom:1px dotted #000000; padding-left:366px;}
.loginTable{ padding-left:216px; width:637px; font-size:14px;}
.loginTable table { margin-top:20px;width:500px;}
.loginTable table td {white-space: nowrap;}
.logintext { border:none; background:url(../images/logininput.jpg) no-repeat;height:34px;line-height:34px !important; overflow: hidden;width:255px;}
.loginicon{ padding-left:100px;}

.mainclose{background:url(../images/fg06.jpg) repeat-x #fff;margin:10px 3px 0 2px;width:997px; overflow:hidden; border-top:1px solid #bbf4fe; border-bottom:1px solid #e0e0e0;border-left:1px solid #e0e0e0;border-right:1px solid #e0e0e0;}
.close_top{ width:997px; height:25px; margin-bottom:10px;}
.close_top_title{width:125px; height:25px; line-height:27px; padding:0 0 0 28px;  display:inline-block;background:url(../images/fg07.jpg) no-repeat; color:#ffffff; font-size:14px; font-weight:bold;}
.close_center{width:997px;}
.close_bd1{ width:997px; height:35px;}
.close_bd1_detail{ width:997px; height:35px; border-bottom:1px solid #e0e0e0; }
.close_bd1_detail dl{ width:997px; height:35px; }
.close_bd1_detail dl:hover{ background:#edf7fd;width:997px; height:35px; cursor:pointer;}
.close_bd1 .close_bd1_detail dt{ height:35px; line-height:35px; float:left;}
.housetitle{ width:288px;height:35px; line-height:35px; text-align:left; margin-left:16px; display:inline;}
.housetitle a{ color:#376eb7;}
.close_bd1 .close_bd1_detail dd{ float:left; height:35px; line-height:35px;}
.road{color:#5f5f5f; width:80px;text-align:right;}
.area{ width:100px; text-align:right;color:#5f5f5f;}
.money{ width:112px;text-align:right; color:#9e9e9e;}
.money strong{ color:#eb640a;}
.check{ width:84px; text-align:right;color:#3586d6; font-size:13px;}
.editor{ background:url(../images/editor.png) no-repeat 40px center; width:80px;text-align:right;}
.editor a{ padding-left:16px; color:#3586d6; }
.delete{ background:url(../images/delete.png) no-repeat 16px center;width:60px;text-align:right;}
.delete a{ padding-left:16px; color:#3586d6;}
.rent{ margin-left:16px; display:inline; background:#f2f2f2; border:1px solid #d1d1d1; margin-top:6px; width:51px; text-align:center; height:20px !important;line-height:20px !important;}
.rent a{ color:#000;}

